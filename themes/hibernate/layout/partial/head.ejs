<head prefix="og: http://ogp.me/ns#">
    <%
    const description = page.description || (page.project ? site.data.projects[page.project].description : null);
    const page_title = url === '/' ? (page.title ? page.title : 'page.simple_name.toUpperCase()') :
            (
                (page.title ? page.title : 'page.simple_name.toUpperCase()') + '-' //+ (page.data.project ? site.data.projects[real_page.project].name : site.title)
            )
    %>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="<%= page.author %>">
    <meta http-equiv="Content-Security-Policy"
          content="default-src 'none'; font-src 'self' data: https://cdn.jsdelivr.net https://static.jboss.org https://in.relation.to https://staging.in.relation.to https://fonts.gstatic.com https://fonts.googleapis.com; style-src 'unsafe-inline' 'self' https://cdn.jsdelivr.net https://fonts.googleapis.com; script-src 'self' 'unsafe-inline' https://hibernate.atlassian.net https://in.relation.to https://staging.in.relation.to https://www.redhat.com https://cdn.jsdelivr.net https://code.jquery.com www.google-analytics.com; img-src 'self' data:  https://*.githubusercontent.com  www.google-analytics.com https://static.jboss.org https://cdn.jsdelivr.net; base-uri 'none'; form-action 'none'; object-src 'none'; connect-src 'self' https://api.github.com; frame-src https://hibernate.atlassian.net">
    <meta http-equiv="X-XSS-Protection" content="1; mode=block">
    <meta http-equiv="X-Content-Type-Options" content="nosniff">

    <meta property="og:site_name" content="<%= site.title %>">
    <meta property="og:url" content="<%= site.base_url %>#<%= page.url %>">
    <meta property="og:title" content="<%= page_title %>">

    <% if (description) { %>
        <meta property="og:description" content="<%= description %>">
        <meta name="description" content="<%= description %>">
    <% } %>

    <meta property="og:image" content="<%- url_for('/images/hibernate_icon_whitebkg.svg') %>">

    <link href="<%=site.data.links.semantic_ui_css_url%><%=site.data.links.minified%>.css" rel="stylesheet"/>
    <link href="<%=site.data.links.slick_css_url%>.css" rel="stylesheet"/>
    <link href="<%=site.data.links.slick_css_url%>-theme.css" rel="stylesheet"/>
    <link href="/css/hibernate.css?t=<%= Date.now() %>" rel="stylesheet"/>
    <!--  Alternative way to include styles:  -->
    <!-- <%- css('css/hibernate.css') %>-->


    <!-- Defines the project favicon. Change these URLs to your specific project folder on static.jboss.org.-->

    <link rel="shortcut icon" href="<%- site.data.links.project_images_url + "/favicon.ico" %>" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="<%- site.data.links.project_images_url+"/apple-touch-icon-144x144-precomposed.png"%>" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="<%- site.data.links.project_images_url+"/apple-touch-icon-114x114-precomposed.png" %>"/>
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="<%- site.data.links.project_images_url + "/apple-touch-icon-72x72-precomposed.png"%>" />
    <link rel="apple-touch-icon-precomposed" href="<%- site.data.links.project_images_url + "/apple-touch-icon-precomposed.png"%>" />

    <script src="<%= site.data.links.jquery_js_url %>.js" crossorigin="anonymous" type="text/javascript" integrity="<%= site.data.links.jquery_js_url_integrity %>"></script>
    <script src="<%= site.data.links.semantic_ui_js_url %>.js" crossorigin="anonymous" type="text/javascript" integrity="<%= site.data.links.semantic_ui_js_url_integrity %>"></script>
    <script src="<%= site.data.links.match_height_js_url %>.js" crossorigin="anonymous" type="text/javascript" integrity="<%= site.data.links.match_height_js_url_integrity %>"></script>

    <% if (page.javascripts) {
        for (const javascript in page.javascripts) {
    %>
        <script src="<%= javascript %>" type='text/javascript'></script>
    <% } }%>


</head>
